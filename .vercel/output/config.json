{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/cron/daily-publish/?$",
			"dest": "_render"
		},
		{
			"src": "^/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/disclaimer/?$",
			"dest": "_render"
		},
		{
			"src": "^/privacy-policy/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/terms-conditions/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}